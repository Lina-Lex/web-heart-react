    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.3",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "antd": "^4.16.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-scripts": "^4.0.3",
    "styled-components": "^5.3.0",
    "styled-media-query": "^2.1.2",
